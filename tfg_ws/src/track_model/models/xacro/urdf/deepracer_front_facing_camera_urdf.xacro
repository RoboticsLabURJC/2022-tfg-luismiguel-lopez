      velocity="100" />
  </joint>
  <link
    name="zed_camera_link">
    <inertial>
      <origin
        xyz="0.019881 6.2875E-05 0.034966"
        rpy="0 0 0" />
      <mass
        value="1e-3" />
      <inertia
        ixx="1E-06"
        ixy="0"
        ixz="0"
        iyy="1E-06"
        iyz="0"
        izz="1E-06" />
    </inertial>
    <xacro:if value="${'deepracer' in shell_val}">
      <visual>
        <origin
          xyz="0 0 0"
          rpy="0 0 0" />
        <geometry>
          <mesh
            filename="package://meshes/zed_camera_link.STL" />
        </geometry>
      </visual>
    </xacro:if>
  </link>
  <joint
    name="zed_camera_joint"
    type="fixed">
    <origin
      xyz="0.091711 0 0.080023"
      rpy="0 0.2618 0" />
    <parent
      link="chassis" />
    <child
      link="zed_camera_link" />
    <axis
      xyz="0 0 0" />
  </joint>
  <link
    name="camera_link">
    <inertial>
      <origin
        xyz="0.0005 0 0"
        rpy="0 0 0" />
      <mass
        value="0.001" />
      <inertia
        ixx="1E-06"
        ixy="0"
        ixz="0"
        iyy="1E-06"
        iyz="0"
        izz="1E-06" />
    </inertial>
  </link>
  <joint
    name="camera_joint"
    type="fixed">
    <origin
      xyz="0.044755 0 0.04"
      rpy="0 0 0" />
    <parent
      link="zed_camera_link" />
    <child
      link="camera_link" />
    <axis
      xyz="0 0 0" />
  </joint>
  <link
    name="shell">
    <inertial>
      <origin
        xyz="0 0 0"
        rpy="0 0 0" />
      <mass
        value="0" />
      <inertia
        ixx="0"
        ixy="0"
        ixz="0"
        iyy="0"
        iyz="0"
        izz="0" />
    </inertial>
    <visual>
      <origin
        xyz="0 0 0"
        rpy="0 0 0" />
      <geometry>
        <mesh
          filename="package://meshes/$(arg shell)" />
      </geometry>
    </visual>
    <collision>
      <origin
        xyz="0 0 0"
        rpy="0 0 0" />
      <geometry>
        <mesh
          filename="package://meshes/$(arg shell)" />
      </geometry>
    </collision>
  </link>
  <joint
    name="shell_joint"
    type="fixed">
    <origin
      xyz="$(arg shell_joint_origin_x) $(arg shell_joint_origin_y) $(arg shell_joint_origin_z)"
      rpy="0 0 0" />
    <parent
      link="chassis" />
    <child
      link="shell" />
    <axis
      xyz="0 0 0" />
  </joint>
</robot>
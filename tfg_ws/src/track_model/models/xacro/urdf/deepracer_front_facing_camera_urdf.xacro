    <child
      link="left_front_wheel" />
    <axis
      xyz="0 1 0" />
    <limit
      lower="-1"
      upper="1"
      effort="10"
      velocity="100" />
  </joint>
  <link
    name="right_steering_hinge">
    <inertial>
      <origin
        xyz="0 0 0"
        rpy="0 0 0" />
        <mass value="0.050" />
      <inertia
        ixx="4E-06"
        ixy="0"
        ixz="0"
        iyy="4E-06"
        iyz="0"
        izz="4E-06" />
    </inertial>
    <visual>
      <origin
        xyz="0 0 0"
        rpy="0 0 0" />
      <geometry>
        <mesh
          filename="package://meshes/right_steering_hinge.STL" />
      </geometry>
    </visual>
  </link>
  <joint
    name="right_steering_hinge_joint"
    type="continuous">
    <origin
      xyz="0.082311 -0.079601 0.011759"

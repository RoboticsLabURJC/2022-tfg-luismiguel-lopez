<robot
  name="deepracer_single_cam" xmlns:xacro="http://www.ros.org/wiki/xacro">
  <link 
    name="base_link" />
  <link
    name="chassis">
    <inertial>
      <origin
        xyz="0 0 0"
        rpy="0 0 0" />
      <mass value="1.9068" />
	    <inertia ixx="0.004151895" ixy="0"             ixz="0"
	                               iyy="0.010379737"   iyz="0"
	                                                   izz="0.010379737" />
    </inertial>
  </link>
  <joint name="base_link_joint" type="fixed">
    <origin xyz="0 0 0.023249" rpy="0 0 0" />
    <parent link="base_link" />
    <child link="chassis" />
  </joint>
  <link
    name="left_rear_wheel">
    <inertial>
      <origin
        xyz="0 0 0"
        rpy="0 1.5707 1.5707" />
     <mass value="0.042222" />
	   <inertia ixx="0.0000611771" ixy="0"            ixz="0"
	                               iyy="0.0000611771" iyz="0"
	                                                  izz="0.0000944606" />        
    </inertial>
    <visual>
      <origin
        xyz="0 0 0"
        rpy="0 1.5707 1.5707" />
      <geometry>
        <cylinder length="0.025" radius="0.03" />
      </geometry>
    </visual>
    <collision>
      <origin
        xyz="0 0 0"
        rpy="0 1.5707 1.5707" />
      <geometry>
        <cylinder length="0.025" radius="0.03" />
      </geometry>

    </collision>
  </link>
  <joint
    name="left_rear_wheel_joint"
    type="continuous">
    <origin
      xyz="-0.081663 0.08105 0.01575"
      rpy="0 0 0" />
    <parent
      link="chassis" />
    <child
      link="left_rear_wheel" />
    <axis
      xyz="0 1 0" />
    <limit
      lower="-1"
      upper="1"
      effort="10"
      velocity="100" />
  </joint>
  <link
    name="right_rear_wheel">
    <inertial>
      <origin

     <mass value="0.042222" />
	   <inertia ixx="0.0000611771" ixy="0"            ixz="0"
	                               iyy="0.0000611771" iyz="0"
	                                                  izz="0.0000944606" />        

    </inertial>
    <visual>
      <origin
        xyz="0 0 0"
        rpy="0 1.5707 1.5707" />
      <geometry>
        <cylinder length="0.025" radius="0.03" />

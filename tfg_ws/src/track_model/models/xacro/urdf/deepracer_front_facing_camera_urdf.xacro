    <parent
      link="chassis" />
    <child
      link="right_steering_hinge" />
    <axis
      xyz="0 0 1" />
    <limit
      lower="-1"
      upper="1"
      effort="10"
      velocity="100" />
  </joint>
  <link
    name="right_front_wheel">
      <inertial>
      <origin
        xyz="0 0 0"
        rpy="0 1.5707 1.5707" />
     <mass value="0.042222" />
	   <inertia ixx="0.0000611771" ixy="0"            ixz="0"
	                               iyy="0.0000611771" iyz="0"
	                                                  izz="0.0000944606" />        

    </inertial>
    <visual>
      <origin
        xyz="0 0 0"
        rpy="0 1.5707 1.5707" />
      <geometry>
        <cylinder length="0.025" radius="0.03" />
      </geometry>
    </visual>
    <collision>
      <origin
        xyz="0 0 0"
        rpy="0 1.5707 1.5707" />
      <geometry>
        <cylinder length="0.025" radius="0.03" />
      </geometry>
    </collision>
  </link>
  <joint
    name="right_front_wheel_joint"
    type="continuous">
    <origin
      xyz="0 0 0"
      rpy="0 0 0" />
    <parent
      link="right_steering_hinge" />
    <child
      link="right_front_wheel" />
    <axis
      xyz="0 1 0" />
    <limit
      lower="-1"
      upper="1"
